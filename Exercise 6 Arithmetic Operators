a = 5
b = 2

print("a =", a)
print("b =", b)
print("")

print(" a + b =", a +b )
print(" a - b =", a - b)
print(" a * b =", a * b)
print ( a ** b =", a ** b)
print(" a / b", a / b)
print(" a % b =", a % b)
print( a // b =", a // b)
print(" a // -b =", a // -b)

c = 4
d = 5

print("")
print("c before =", c)
print("d before +", d)

c *= d

print(" c after =", c)

result = 5 * 2 + 8

print("result +", result)
